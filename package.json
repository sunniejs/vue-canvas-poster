{
  "name": "vue-canvas-poster",
  "version": "1.2.1",
  "description": "A lightweight vue components use canvas draw image by css properties.(一个通过css属性画canvas图片的轻量级的vue组件)",
  "author": "Sunnie <sunniejs@163.com>",
  "license": "MIT",
  "main": "./dist/vue-canvas-poster.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunniejs/vue-canvas-poster"
  },
  "bugs": {
    "url": "https://github.com/sunniejs/vue-canvas-poster/issues"
  },
  "homepage": "https://github.com/sunniejs/vue-canvas-poster#readme",
  "keywords": [
    "vue canvas",
    "canvas image",
    "poster",
    "wechat"
  ],
  "private": false,
  "scripts": {
    "dev": "npm run serve",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name vue-canvas-poster ./src/lib/index.js"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "semi": [
        2,
        "never"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
